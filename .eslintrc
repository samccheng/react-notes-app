{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6
    },
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "meteor": true
    },
  "rules": {
    "comma-dangle": 0,
    "react/jsx-filename-extension": 0,
    "semi": 0,
    "import/no-unresolved": 0
    },
  "plugins": [
    "react",
    "jsx-a11y"
  ]
}
